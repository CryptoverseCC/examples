{
  "name": "governance",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://userfeeds.github.io/examples/examples/governance/",
  "title": "Governance",
  "description": "See what are the incentives behind the votes.",
  "img": "assets/examples-governance.svg",
  "dependencies": {
    "bulma": "^0.7.1",
    "react": "^16.3.2",
    "react-blockies": "^1.3.0",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/App.scss -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/App.scss -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  }
}
